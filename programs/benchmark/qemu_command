1. Ausführung des unmodifizierten Kernels
qemu-system-x86_64 \
  -kernel <kernel>/arch/x86/boot/bzImage \
  -initrd <busybox>/initramfs.cpio.gz \
  -append "console=ttyS0" \
  -nographic \
  -m 6G \
  -enable-kvm \
  -cpu host \

2. Ausführung mmap-autoselct Kernel
qemu-system-x86_64 \
  -kernel <kernel>/arch/x86/boot/bzImage \
  -initrd <busybox>/initramfs.cpio.gz \
  -append "console=ttyS0 hugepagesz=1GB hugepages=2" \
  -nographic \
  -m 6G \
  -enable-kvm \
  -cpu host\

